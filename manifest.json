{
    "name": "MangaStream Feed Reader",
    "version": "1.0",
    "description": "Extensão para listar as últimas postagens do site MangaStream",
    "browser_action":   {
        "default_icon": "images/manga.jpg",
        "default_popup": "d_popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/services/feed/load; object-src 'self'",
    "manifest_version": 2,
    "icons":{
        "128":"images/manga.jpg"
    }
}